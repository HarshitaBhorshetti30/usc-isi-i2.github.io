Urlpaper = {http://www.isi.edu/~szekely/contents/papers/1989/standardizing-interface-UIST-1989.pdf},
uri = {\url{papers2://publication/uuid/E7D126C8-1D0B-4B69-980C-694767ED1C09}}
}

