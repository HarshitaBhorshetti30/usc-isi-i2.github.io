date-added = {2013-01-18T02:06:42GMT},
date-modified = {2013-05-02T16:22:57GMT},
abstract = {We describe MASTERMIND, a step toward our vision of a knowledge-based design-time and run-time environment in which human-computer interfaces development is centered around an all-encompassing design model. The MASTERMIND approach is intended to provide integration and continuity across the entire life cycle of the user interface. In addition, it facilitates higher quality work within each phase of the life cycle. MASTERMIND is an open framework, in which the design knowledge base allows multiple tools to come into play and makes knowledge created by each tool accessible to the others.},
Urlpaper = {http://www.isi.edu/~szekely/contents/papers/1993/knowledgeable-development.pdf},
uri = {\url{papers2://publication/uuid/6D339538-B8CA-441F-BFC1-2A10B05C18FB}}
}

