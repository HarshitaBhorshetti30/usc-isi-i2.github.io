@inproceedings{Castells:1999,
author = {Castells, Pablo and Szekely, Pedro A},
title = {{Presentation Models by Example}},
booktitle = {Design, Specification and Verification of Interactive Systems'99, Proceedings of the Eurographics Workshop in Braga, Portugal, June 2-4, 1999},
year = {1999},
