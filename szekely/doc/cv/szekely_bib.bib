@inproceedings{Puerta:1994a,
author = {Puerta, Angel R and Neches, Robert and Eriksson, Henrik and Szekely, Pedro and Luo, Ping and Musen, Mark A},
title = {{Toward Ontology-Based Frameworks for Knowledge-Acquisition Tools}},
booktitle = {In Proceedings of the Eigth KnowledgeAcquisition Workshop for Knowledge-Based Systems},
year = {1994},
annote = {Angel R. Puerta (USC/Information Sciences Institute; Admiralty Way; Marina del Rey , CA 90292 , USA); Henrik Eriksson (USC/Information Sciences Institute; Admiralty Way; Marina del Rey , CA 90292 , USA); Pedro Szekely (USC/Information Sciences Institute; Admiralty Way; Marina del Rey , CA 90292 , USA); Ping Luo (USC/Information Sciences Institute; Admiralty Way; Marina del Rey , CA 90292 , USA); Mark A. Musen (USC/Information Sciences Institute; Admiralty Way; Marina del Rey , CA 90292 , USA);},
language = {English},
rating = {0},
date-added = {2013-01-18T02:06:42GMT},
date-modified = {2013-05-02T16:22:57GMT},
abstract = {One of the strongest limitations of knowledge-acquisition metatools---tools that automate the development of knowledge-acquisition tools---is that the interface-design knowledge employed by such metatools to produce domain-specific knowledge-acquisition tools is not explicit and cannot be used by other metatools. Therefore, the usefulness of a metatool is restricted to its own implementation environment. A possible solution to this problem is the definition of a shareable ontology of knowledge-acquisition tools that represents explicitly the relevant knowledge of tool design and development. We take the position that the design of knowledge-acquisition tools is similar to the design of user interfaces, and that an ontology of knowledge-acquisition tools is a special case of an interface ontology. We propose that a shareable interface ontology can be defined by analyzing interface ontologies used in model-based user-interface development systems, and by resolving differences and conflicts among the ontologies---a process called model alignment. We present the initial results of a model alignment for the interface ontologies of two user-interface development systems: SHELTER and Mecano. We also show how the resulting shareable interface ontology underpins a framework in which each system can be applied to different parts of the process of generating a single, domain-specific knowledge-acquisition tool, thus creating a generation of metatools useful across multiple implementation environments.},
Urlpaper = {http://www.isi.edu/~szekely/contents/papers/1994/shelter-mecano-KA-workshop-1994.pdf},
uri = {\url{papers2://publication/uuid/C2CBFD1A-462B-44B5-B936-0B58A00D3360}}
}

@inproceedings{Neches:1993a,
author = {Neches, Robert and Foley, Jim and Szekely, Pedro and Sukaviriya, Piyawadee and Luo, Ping and Kovacevic, Srdjan and Hudson, Scott},
title = {{Knowledgeable Development Environments Using Shared Design Models}},
booktitle = {Proceedings of the 1993 International Workshop on Intelligent User Interfaces},
year = {1993},
pages = {63--70},
