@inproceedings{Szekely:1995,
author = {Szekely, Pedro and Sukaviriya, Piyawadee Noi and Castells, Pablo and Muthukumarasamy, Jeyakumar and Salcher, Ewald},
title = {{Declarative Interface Models for User Interface Construction Tools: the MASTERMIND Approach}},
booktitle = {Engineering for Human Computer Interaction (EHCI)},
year = {1995},
